<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>com.github.driver733</groupId>
        <artifactId>jcabi-parent</artifactId>
        <version>8864b83e8e</version>
    </parent>
    <repositories>
        <repository>
            <id>jitpack.io</id>
            <url>https://jitpack.io</url>
        </repository>
    </repositories>
    <groupId>groupId</groupId>
    <artifactId>VKMusicUploader</artifactId>
    <version>1.0-SNAPSHOT</version>
    <dependencies>
        <dependency>
            <groupId>com.github.driver733</groupId>
            <artifactId>vk-java-sdk</artifactId>
            <version>547a05fa42</version>
        </dependency>
        <dependency>
            <groupId>org.apache.commons</groupId>
            <artifactId>commons-lang3</artifactId>
            <version>3.5</version>
        </dependency>
        <dependency>
            <groupId>com.jcabi</groupId>
            <artifactId>jcabi-immutable</artifactId>
            <version>1.4</version>
        </dependency>
        <dependency>
            <groupId>org.apache.commons</groupId>
            <artifactId>commons-collections4</artifactId>
            <version>4.1</version>
        </dependency>
        <dependency>
            <groupId>commons-io</groupId>
            <artifactId>commons-io</artifactId>
            <version>2.5</version>
        </dependency>
        <dependency>
            <groupId>org.apache.httpcomponents</groupId>
            <artifactId>httpclient</artifactId>
            <version>4.5.2</version>
        </dependency>
        <dependency>
            <groupId>org.apache.httpcomponents</groupId>
            <artifactId>httpmime</artifactId>
            <version>4.5.2</version>
        </dependency>
        <dependency>
            <groupId>com.mpatric</groupId>
            <artifactId>mp3agic</artifactId>
            <version>0.9.0</version>
        </dependency>
        <dependency>
            <groupId>com.google.code.gson</groupId>
            <artifactId>gson</artifactId>
            <version>2.8.0</version>
        </dependency>
        <dependency>
            <groupId>org.slf4j</groupId>
            <artifactId>slf4j-jdk14</artifactId>
            <version>1.7.25</version>
        </dependency>
        <dependency>
            <groupId>com.jcabi</groupId>
            <artifactId>jcabi-aspects</artifactId>
        </dependency>
        <dependency>
            <groupId>org.aspectj</groupId>
            <artifactId>aspectjrt</artifactId>
            <scope>runtime</scope>
        </dependency>
        <dependency>
            <groupId>javax.validation</groupId>
            <artifactId>validation-api</artifactId>
            <scope>runtime</scope>
        </dependency>
        <dependency>
            <groupId>com.github.driver733.qulice</groupId>
            <artifactId>jaxen</artifactId>
            <version>0.17.1</version>
        </dependency>
    </dependencies>
    <build>
        <pluginManagement>
            <plugins>
                <plugin>
                    <groupId>com.qulice</groupId>
                    <artifactId>qulice-maven-plugin</artifactId>
                    <configuration>
                        <license>origin:${basedir}/LICENSE_qulice.txt</license>
                        <excludes>
                            <exclude>checkstyle:/src/main/java/com.driver733.vkmusicuploader/Main.java</exclude>
                            <exclude>pmd:/src/main/java/com.driver733.vkmusicuploader/Main.java</exclude>
                            <exclude>findbugs:com.driver733.vkmusicuploader.Main</exclude>

                            <exclude>findbugs:com.driver733.vkmusicuploader.support.CachedExecuteBatchQueryClient</exclude>
                            <exclude>findbugs:com.driver733.vkmusicuploader.support.ImmutableProperties</exclude>

                            <exclude>checkstyle:/src/main/java/com/driver733/vkmusicuploader/support/CachedExecuteBatchQueryClient.java</exclude>
                            <exclude>pmd:/src/main/java/com/driver733/vkmusicuploader/support/CachedExecuteBatchQueryClient.java</exclude>
                            <exclude>checkstyle:/src/main/java/com/driver733/vkmusicuploader/support/ConnectionsSupervisor.java</exclude>
                            <exclude>pmd:/src/main/java/com/driver733/vkmusicuploader/support/ConnectionsSupervisor.java</exclude>

                            <exclude>xml:/src/main/resources/.*</exclude>
                            <exclude>duplicatefinder:.*</exclude>
                            <exclude>cobertura:.*</exclude>
                            <exclude>dependencies:.*</exclude>
                        </excludes>
                    </configuration>
                </plugin>
                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-compiler-plugin</artifactId>
                    <configuration>
                        <source>1.8</source>
                        <target>1.8</target>
                    </configuration>
                </plugin>
            </plugins>
        </pluginManagement>
    </build>
</project>
